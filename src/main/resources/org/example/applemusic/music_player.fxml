<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="166.0" prefWidth="287.0" spacing="10" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.applemusic.MusicPlayerController">
   <AnchorPane prefHeight="200.0" prefWidth="200.0">
      <children>
         <Label fx:id="status" alignment="CENTER" layoutX="22.0" layoutY="11.0" prefHeight="42.0" prefWidth="238.0" text="Music PLayer">
            <font>
               <Font name="Arial" size="13.0" />
            </font>
         </Label>
          <Button fx:id="pause" disable="true" layoutX="154.0" layoutY="57.0" onAction="#pause" prefHeight="50.0" prefWidth="65.0" text="Pause" />
          <Button fx:id="play" alignment="CENTER" contentDisplay="TOP" disable="false" layoutX="76.0" layoutY="58.0" onAction="#play" prefHeight="49.0" prefWidth="38.0" text="Play" />
      </children>
   </AnchorPane>
</VBox>
